				"source": [
					"Dim_Depo = spark.sql(\"\"\"\r\n",
					"\r\n",
					"SELECT id as `depo.id`, \r\n",
					"\t   name as `depo.name`, \r\n",
					"\t   depcode AS `dispatch.site`, \r\n",
					"\t   address AS `depo.address`, \r\n",
					"\t   suburb AS `depo.suburb`, \r\n",
					"\t   city AS `depo.city`, \r\n",
					"\t   zipcode AS `depo.zipcode`, \r\n",
					"\t   region AS `depo.region`, \r\n",
					"\t   lat AS `depo.lat`, \r\n",
					"\t   lng AS `depo.lng`, \r\n",
					"\t   placeid AS `depo.placeid`, \r\n",
					"\t   w3w AS `depo.w3w`, \r\n",
					"\t   insdate AS `depo.insdate`, \r\n",
					"\t   `update` AS `depo.update`, \r\n",
					"\t   lmsid AS `depo.lmsid`, \r\n",
					"\t   country AS `depo.country`, \r\n",
					"\t   status AS `depo.status`\r\n",
					"FROM publicdepo\r\n",
					"\r\n",
					"\"\"\")\r\n",
					"#Dim_Depo.show()\r\n",
					"Dim_Depo.createOrReplaceTempView(\"Dim_Depo\")"
				],
				"execution_count": 5
			},
