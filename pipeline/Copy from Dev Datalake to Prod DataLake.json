{
	"name": "Copy from Dev Datalake to Prod DataLake",
	"properties": {
		"activities": [
			{
				"name": "Copy_03f",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "synapse/Unstructured Data/"
					},
					{
						"name": "Destination",
						"value": "synapse/Unstructured Data/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "Unstructured Data",
							"wildcardFileName": "*",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"skipErrorFile": {
						"fileMissing": true
					},
					"validateDataConsistency": false,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "citylogistics-synapseanalytics-workspace-prod-WorkspaceDefaultStorage",
								"type": "LinkedServiceReference"
							},
							"path": "synapse/Logging/"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_03f",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_03f",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}