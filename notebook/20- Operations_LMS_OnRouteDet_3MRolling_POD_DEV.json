					"collapsed": false
				},
				"source": [
					"pod = spark.sql(\"\"\"\r\n",
					"SELECT id as `pod.id`,\r\n",
					"       did as `delivery.id`,\r\n",
					"       name as `pod.name`,\r\n",
					"       poddate as `pod.poddate`,\r\n",
					"       CONCAT('(',lat,' ',lng,')') as `pod.geopoint`,\r\n",
					"       lat as `pod.lat`,\r\n",
					"       lng as `pod.lng`,\r\n",
					"       customerrating as `pod.customerrating`,\r\n",
					"       customerfeedback as `pod.customerfeedback`\r\n",
					"FROM publicpod\r\n",
					"\"\"\")\r\n",
					"#pod.show()\r\n",
					"pod.createOrReplaceTempView(\"pod\")"
				],
				"execution_count": 2
			},
