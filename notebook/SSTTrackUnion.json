				"source": [
					"%%sql\r\n",
					"CREATE OR REPLACE TEMP VIEW\r\n",
					"ssttrackunion\r\n",
					"AS\r\n",
					"SELECT t.* \r\n",
					"    ,p.weight\r\n",
					"    ,p.chargeweight\r\n",
					"    ,p.volweight\r\n",
					"    ,p.volumiserweight\r\n",
					"    ,p.PickUpCustID\r\n",
					"    ,p.DeliverCustID\r\n",
					"    ,p.BillCustID\r\n",
					"    ,p.specdel\r\n",
					"    \r\n",
					"FROM dbotrackunion t\r\n",
					"LEFT JOIN sstparcelunion p on p.id = t.parcelid\r\n",
					""
				],
