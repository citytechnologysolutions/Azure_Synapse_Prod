				"source": [
					"Dim_orvuser = spark.sql(\r\n",
					"\"\"\"\r\n",
					"    select \r\n",
					"\r\n",
					"    id as `orvuser.id`,\r\n",
					"    id as `dispatch.did`,\r\n",
					"    username as `orvuser.username`,\r\n",
					"    email as `orvuser.email`,\r\n",
					"    fname as `orvuser.fname`,\r\n",
					"    lname as `orvuser.lname`,\r\n",
					"    CONCAT(fname, ' ',  lname) as `orvuser.fullname`,\r\n",
					"    idpassportno as `orvuser.idpassportno`,\r\n",
					"    termindate as `orvuser.termindate`,\r\n",
					"    status as `orvuser.status`,\r\n",
					"    depot as `orvuser.depot`,\r\n",
					"    jobcategory as `orvuser.jobcategory`\r\n",
					"\r\n",
					"    from publicorvuser\r\n",
					"\r\n",
					"    --where id > ${vMaxidorvuser}\r\n",
					"\"\"\")\r\n",
					"Dim_orvuser.createOrReplaceTempView(\"Dim_orvuser\")\r\n",
					""
