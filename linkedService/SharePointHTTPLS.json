{
	"name": "SharePointHTTPLS",
	"properties": {
		"parameters": {
			"BaseUrl": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			},
			"SiteURL": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{concat(linkedService().BaseUrl, linkedService().SiteURL, '_api/web/GetFileByServerRelativeUrl (''''', linkedService().FileName,''''')/$value)')}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	}
}